<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="deviceTrackDao" class="org.zenses.data.dao.DeviceTrackDaoImpl" />
	<bean id="deviceTrackService" class="org.zenses.data.service.DeviceTrackServiceImpl" />
	<bean id="lastFmTracksSubmitter" class="org.zenses.lastfm.LastFmTracksSubmitter">
		<property name="lastfmPassword" value="${lastfm.password}" />
		<property name="lastfmUser" value="${lastfm.user}" />
	</bean>
	<bean id="trackSubmitter" class="org.zenses.lastfm.TracksSubmitterImpl" />

</beans>
